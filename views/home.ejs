<%- include("./partials/header") -%>
<h1>HOME</h1>

<p><%= homeStart %></p>



<% posts.forEach(function(post) { %>
    <hr>
    <h4><%=post.date%></h4>
    <h3><%=post.title%></h3>
    <p><%=post.body.substring(0, 100) + '... '%><a href="/posts/<%=post.title%>">Read more</a></p>
<% }) %>


